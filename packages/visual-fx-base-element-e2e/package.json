{
  "name": "visual-fx-base-element-e2e",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "tsc": "pnpm exec tsc",
    "run-s": "pnpm exec run-s -s",
    "playwright": "pnpm exec playwright",
    "vite": "pnpm exec vite",
    "dev": "pnpm vite",
    "build": "pnpm tsc && pnpm vite build",
    "vite:preview": "pnpm vite preview --host",
    "preview": "pnpm run-s build vite:preview",
    "clean": "pnpm exec rimraf build dist playwright-report coverage test-results",
    "playwright:test": "pnpm playwright test",
    "playwright:test:ui": "pnpm playwright test --ui",
    "playwright:show-report": "pnpm playwright show-report",
    "jest": "pnpm exec jest",
    "test": "pnpm jest && pnpm playwright:test"
  },
  "dependencies": {
    "@spearwolf/visual-fx-base-element": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
